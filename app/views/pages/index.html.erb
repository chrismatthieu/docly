<h1>Listing pages</h1>

<table>
  <tr>
    <th>Pagename</th>
    <th>Pagetitle</th>
    <th>Pagebody</th>
    <th>Password digest</th>
    <th>Pagesecured</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @pages.each do |page| %>
  <tr>
    <td><%= page.pagename %></td>
    <td><%= page.pagetitle %></td>
    <td><%= page.pagebody %></td>
    <td><%= page.password_digest %></td>
    <td><%= page.pagesecured %></td>
    <td><%= link_to 'Show', page %></td>
    <td><%= link_to 'Edit', edit_page_path(page) %></td>
    <td><%= link_to 'Destroy', page, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Page', new_page_path %>
